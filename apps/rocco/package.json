{
  "name": "@hominem/rocco",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "clean": "rm -rf build",
    "build:analyze": "ANALYZE=true react-router build",
    "dev": "react-router dev",
    "docker:start": "../scripts/docker-start.sh",
    "docker:stop": "docker-compose down",
    "docker:restart": "docker-compose restart",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v",
    "format": "bunx oxfmt ./app",
    "lint": "bunx oxlint ./app",
    "preview": "react-router build && react-router-serve ./build/server/index.js",
    "start": "react-router-serve ./build/server/index.js",
    "test": "NODE_ENV=test vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:view": "npx vite preview --outDir coverage/lcov-report",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "typegen": "react-router typegen"
  },
  "dependencies": {
    "@hominem/auth": "workspace:*",
    "@hominem/env": "workspace:*",
    "@hominem/hono-client": "workspace:*",
    "@hominem/hono-rpc": "workspace:*",
    "@hominem/invites-services": "workspace:*",
    "@hominem/lists-services": "workspace:*",
    "@hominem/places-services": "workspace:*",
    "@hominem/services": "workspace:*",
    "@hominem/ui": "workspace:*",
    "@hominem/utils": "workspace:*",
    "@radix-ui/react-dialog": "^1.1.15",
    "@react-router/node": "^7.12.0",
    "@react-router/serve": "^7.12.0",
    "@tanstack/react-query": "5.90.16",
    "@vis.gl/react-google-maps": "^1.7.1",
    "clsx": "^2.1.1",
    "isbot": "^5",
    "lucide-react": "0.562.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-router": "7.12.0",
    "workbox-window": "^7.4.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.12.0",
    "@tailwindcss/vite": "^4.1.18",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.19.3",
    "@types/react": "19.1.17",
    "@types/react-dom": "19.1.11",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/coverage-v8": "4.0.16",
    "dotenv": "17.2.3",
    "jsdom": "^25.0.1",
    "msw": "^2.12.7",
    "rollup-plugin-visualizer": "6.0.5",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.9.3",
    "vite": "7.3.1",
    "vite-plugin-pwa": "^1.2.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  }
}
