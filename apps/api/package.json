  "name": "@hominem/api",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "pnpm tsx --watch src/index.ts",
    "lint": "pnpm biome lint ./src --fix",
    "format": "pnpm biome format ./src --fix",
    "start": "pnpm node --import tsx src/index.ts | pnpm pino-pretty",
    "build": "pnpm exec node esbuild.config.js",
    "start:prod": "pnpm node dist/index.js",
    "test": "NODE_ENV=test pnpm vitest --run --coverage --passWithNoTests",
    "test:coverage": "NODE_ENV=test pnpm vitest --run --coverage",
    "test:watch": "NODE_ENV=test pnpm vitest",
    "clean": "rm -rf dist",
    "prebuild": "pnpm clean"
  },
