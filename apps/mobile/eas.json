{
  "cli": {
    "version": ">= 10.0.3"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "https://auth.ponti.io",
        "EXPO_PUBLIC_E2E_TESTING": "false"
      }
    },
    "simulator": {
      "channel": "development",
      "developmentClient": true,
      "env": {
        "NODE_ENV": "development",
        "EXPO_PUBLIC_API_BASE_URL": "http://localhost:4040",
        "EXPO_PUBLIC_E2E_TESTING": "true"
      },
      "extends": "development",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "channel": "preview",
      "distribution": "internal"
    },
    "production": {
      "channel": "production",
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_API_BASE_URL": "https://auth.ponti.io"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
