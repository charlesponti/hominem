    "from typing import Callable\n",
    "\n",
    "def do_twice(func: Callable[[str], str], argument: str) -> None:\n",
    "  print(func(argument))\n",
    "  print(func(argument))\n",
    "\n",
    "def greet(name: str) -> str:\n",
    "  return f\"Hello {name}\"\n",
    "\n",
    "\n",
    "do_twice(greet, \"Jekyll\")"
