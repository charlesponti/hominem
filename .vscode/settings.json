{
  "typescript.tsserver.enableTracing": true,
  "typescript.tsserver.log": "normal",
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/**": true,
    "**/dist/**": true,
    "**/build/**": true,
    "**/.cache/**": true,
    "**/.tsbuildinfo": true,
    "**/.turbo/**": true,
    "**/.react-router/cache/**": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/build": true,
    "**/dist": true,
    "**/.cache": true,
    "**/.turbo": true
  }
}
