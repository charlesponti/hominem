{
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "biome.enabled": true,
  "biome.lspBin": "node_modules/.bin/biome",
  "biome.searchInPath": true,
  "cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/{info,lfs,logs,refs,objects}/**",
    ".git/{index,*refs,*HEAD}",
    ".vscode",
    ".vscode-insiders",
    ".env*"
  ],
  "cSpell.ignoreWords": [
    "openai",
    "langchain",
    "fastify"
  ],
  "cSpell.words": [
    "AUTOINCREMENT",
    "biomejs",
    "Checkpointer",
    "chromadb",
    "chrono",
    "googleauth",
    "healthcheck",
    "initdb",
    "langchain",
    "langgraph",
    "libsql",
    "ollama",
    "onnxruntime",
    "openai",
    "Peloton",
    "pgadmin",
    "pino",
    "ponti",
    "posttest",
    "prepush",
    "rabbitmq",
    "sendgrid",
    "Serp",
    "serpapi",
    "shadcn",
    "supabase",
    "tavily",
    "typecheck",
    "vectorstores",
    "vercel",
    "xenova"
  ],
  "editor.defaultFormatter": "biomejs.biome",
  "editor.formatOnSave": true,
  "editor.formatOnType": true,
  "rust-analyzer.completion.autoimport.enable": true,
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.tsdk": "node_modules/typescript/lib",
  "[json]": {
    "editor.defaultFormatter": "biomejs.biome"
  }
}