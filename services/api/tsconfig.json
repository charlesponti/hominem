{
  "extends": "../../tsconfig.profiles/service-noemit.json",
  "compilerOptions": {
    "baseUrl": ".",
    "tsBuildInfoFile": "../../.cache/service-api.tsbuildinfo",
    "rootDir": ".",
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": ["src/**/*.ts", "test/**/*.ts", "vitest.config.mts"],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    ".turbo",
    ".cache",
    "coverage",
    "*.tsbuildinfo",
    ".react-router/cache"
  ]
}
