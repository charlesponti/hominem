{
  "extends": "../../tsconfig.profiles/service-noemit.json",
  "compilerOptions": {
    "tsBuildInfoFile": "../../.cache/service-workers.tsbuildinfo",
    "rootDir": "."
  },
  "include": ["src/**/*.ts", "vitest.config.ts"],
  "exclude": [
    "node_modules",
    "dist",
    "build",
    ".turbo",
    ".cache",
    "coverage",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/__tests__",
    "**/__mocks__",
    "*.tsbuildinfo",
    ".react-router/cache"
  ]
}
