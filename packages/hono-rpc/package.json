{
  "name": "@hominem/hono-rpc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./client": "./src/client.ts",
    "./types": "./src/types/index.ts",
    "./types/*": "./src/types/*.ts",
    "./routes/*": "./src/routes/*.ts",
    "./middleware/*": "./src/middleware/*.ts"
  },
  "scripts": {
    "typecheck": "bunx tsc --noEmit --incremental",
    "build": "tsc -b tsconfig.json",
    "dev": "bunx tsc --noEmit --incremental --watch",
    "clean": "rm -rf build node_modules/.cache"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.0",
    "@ai-sdk/openai": "^1.1.0",
    "@ai-sdk/openai-compatible": "^0.1.0",
    "@hominem/auth": "workspace:*",
    "@hominem/chat-services": "workspace:*",
    "@hominem/db": "workspace:*",
    "@hominem/events-services": "workspace:*",
    "@hominem/finance-services": "workspace:*",
    "@hominem/health-services": "workspace:*",
    "@hominem/lists-services": "workspace:*",
    "@hominem/notes-services": "workspace:*",
    "@hominem/places-services": "workspace:*",
    "@hominem/services": "workspace:*",
    "@hominem/utils": "workspace:*",
    "@hono/zod-validator": "^0.7.6",
    "@supabase/supabase-js": "2.88.0",
    "@tanstack/ai": "^0.2.0",
    "ai": "^4.3.0",
    "dotenv": "^16.0.0",
    "drizzle-orm": "0.45.1",
    "hono": "4.11.6",
    "plaid": "^28.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "typescript": "5.9.3"
  }
}
