{
  "name": "@hominem/hono-rpc",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./src/index.ts"
    },
    "./client": {
      "types": "./build/client.d.ts",
      "default": "./src/client.ts"
    },
    "./types": {
      "types": "./build/types/index.d.ts",
      "default": "./src/types/index.ts"
    },
    "./types/*": {
      "types": "./build/types/*.d.ts",
      "default": "./src/types/*.ts"
    },
    "./routes/*": {
      "types": "./build/routes/*.d.ts",
      "default": "./src/routes/*.ts"
    },
    "./middleware/*": {
      "types": "./build/middleware/*.d.ts",
      "default": "./src/middleware/*.ts"
    }
  },
  "scripts": {
    "build": "bunx tsc --incremental",
    "typecheck": "bunx tsc --noEmit --incremental",
    "clean": "rm -rf dist build node_modules/.cache"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@ai-sdk/openai-compatible": "^0.1.0",
    "@hominem/auth": "workspace:*",
    "@hominem/chat-services": "workspace:*",
    "@hominem/db": "workspace:*",
    "@hominem/env": "workspace:*",
    "@hominem/events-services": "workspace:*",
    "@hominem/finance-services": "workspace:*",
    "@hominem/health-services": "workspace:*",
    "@hominem/lists-services": "workspace:*",
    "@hominem/notes-services": "workspace:*",
    "@hominem/places-services": "workspace:*",
    "@hominem/services": "workspace:*",
    "@hominem/utils": "workspace:*",
    "@hono/zod-validator": "^0.7.6",
    "@supabase/supabase-js": "2.88.0",
    "ai": "^4.3.0",
    "hono": "4.11.10",
    "plaid": "^28.0.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/node": "^22.15.2",
    "typescript": "5.9.3"
  }
}
