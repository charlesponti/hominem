{
  "name": "@hominem/services",
  "version": "0.0.0",
  "description": "Business logic and services",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./emails": "./src/resend.ts",
    "./resend": "./src/resend.ts",
    "./files": "./src/files.ts",
    "./fixtures": "./src/fixtures.ts",
    "./google-calendar": "./src/google.ts",
    "./logger": "./src/logger.ts",
    "./people": "./src/people.service.ts",
    "./tags": "./src/tags.service.ts",
    "./env": "./src/env.ts",
    "./queues": "./src/queues.ts",
    "./redis": "./src/redis.ts",
    "./test-utils/google-api-mocks": "./src/test-utils/google-api-mocks.ts",
    "./travel": "./src/travel/index.ts",
    "./vector": "./src/vector.ts",
    "./package.json": "./package.json"
  },
  "scripts": {
    "clean": "rm -rf build",
    "typecheck": "tsc --noEmit",
    "format": "bunx oxfmt ./src",
    "lint": "bunx oxlint ./src",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/openai": "^1.3.22",
    "@hominem/db": "workspace:*",
    "@hominem/utils": "workspace:*",
    "bullmq": "5.66.4",
    "csv-parser": "^3.2.0",
    "dotenv": "17.2.3",
    "googleapis": "^170.0.0",
    "ioredis": "5.8.2",
    "mammoth": "^1.9.1",
    "ai": "^4.3.19",
    "openai": "4.83.0",
    "pdf2json": "^3.1.6",
    "resend": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
