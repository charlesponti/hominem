{
  "name": "@hominem/services",
  "version": "0.0.0",
  "description": "Business logic and services",
  "type": "module",
  "sideEffects": false,
  "exports": {
    "./emails": {
      "types": "./build/resend.d.ts",
      "development": "./src/resend.ts",
      "default": "./build/resend.js"
    },
    "./files": {
      "types": "./build/files.d.ts",
      "development": "./src/files.ts",
      "default": "./build/files.js"
    },
    "./fixtures": {
      "types": "./build/fixtures.d.ts",
      "development": "./src/fixtures.ts",
      "default": "./build/fixtures.js"
    },
    "./google-calendar": {
      "types": "./build/google.d.ts",
      "development": "./src/google.ts",
      "default": "./build/google.js"
    },
    "./logger": {
      "types": "./build/logger.d.ts",
      "development": "./src/logger.ts",
      "default": "./build/logger.js"
    },
    "./google-places": {
      "types": "./build/google-places.service.d.ts",
      "development": "./src/google-places.service.ts",
      "default": "./build/google-places.service.js"
    },
    "./people": {
      "types": "./build/people.service.d.ts",
      "development": "./src/people.service.ts",
      "default": "./build/people.service.js"
    },
    "./tags": {
      "types": "./build/tags.service.d.ts",
      "development": "./src/tags.service.ts",
      "default": "./build/tags.service.js"
    },
    "./env": {
      "types": "./build/env.d.ts",
      "development": "./src/env.ts",
      "default": "./build/env.js"
    },
    "./queues": {
      "types": "./build/queues.d.ts",
      "development": "./src/queues.ts",
      "default": "./build/queues.js"
    },
    "./redis": {
      "types": "./build/redis.d.ts",
      "development": "./src/redis.ts",
      "default": "./build/redis.js"
    },
    "./travel": {
      "types": "./build/travel/index.d.ts",
      "development": "./src/travel/index.ts",
      "default": "./build/travel/index.js"
    },
    "./types": {
      "types": "./build/types.d.ts",
      "development": "./src/types.ts",
      "default": "./build/types.js"
    },
    "./vector": {
      "types": "./build/vector.d.ts",
      "development": "./src/vector.ts",
      "default": "./build/vector.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc -d",
    "typecheck": "tsc --noEmit",
    "dev": "tsc -d -w",
    "format": "bunx oxfmt ./src",
    "lint": "bunx oxlint ./src",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@hominem/db": "workspace:*",
    "@hominem/utils": "workspace:*",
    "ai": "^4.3.16",
    "better-auth": "^1.4.12",
    "bullmq": "5.66.4",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.45.1",
    "drizzle-zod": "^0.7.0",
    "googleapis": "^170.0.0",
    "ioredis": "5.9.1",
    "langchain": "^0.3.15",
    "mammoth": "^1.9.1",
    "openai": "4.83.0",
    "pdf2json": "^3.1.6",
    "postgres": "^3.4.4",
    "resend": "^4.0.1",
    "sharp": "^0.32.4",
    "uuid": "^11.1.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@supabase/supabase-js": "2.88.0",
    "@types/uuid": "^10.0.0",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
