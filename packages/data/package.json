{
  "name": "@hominem/data",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "description": "Database schemas and utilities",
  "exports": {
    "./db": {
      "import": "./src/db/index.ts",
      "types": "./build/db/index.d.ts"
    },
    "./emails": {
      "import": "./src/resend.ts",
      "types": "./build/resend.d.ts"
    },
    "./events": {
      "import": "./src/events/index.ts",
      "types": "./build/events/index.d.ts"
    },
    "./files": {
      "import": "./src/files.ts",
      "types": "./build/files.d.ts"
    },
    "./finance": {
      "import": "./src/finance/index.ts",
      "types": "./build/finance/index.d.ts"
    },
    "./fixtures": {
      "import": "./src/fixtures.ts",
      "types": "./build/fixtures.d.ts"
    },
    "./google-calendar": {
      "import": "./src/google.ts",
      "types": "./build/google.d.ts"
    },
    "./health": {
      "import": "./src/health/index.ts",
      "types": "./build/health/index.d.ts"
    },
    "./lists": {
      "import": "./src/lists/index.ts",
      "types": "./build/lists/index.d.ts"
    },
    "./logger": {
      "import": "./src/logger.ts",
      "types": "./build/logger.d.ts"
    },
    "./places": {
      "import": "./src/places.ts",
      "types": "./build/places.d.ts"
    },
    "./notes": {
      "import": "./src/notes/index.ts",
      "types": "./build/notes/index.d.ts"
    },
    "./schema": {
      "import": "./src/db/schema/index.ts",
      "types": "./build/db/schema/index.d.ts"
    },
    "./services": {
      "import": "./src/services/index.ts",
      "types": "./build/services/index.d.ts"
    },
    "./queues": {
      "import": "./src/queues.ts",
      "types": "./build/queues.d.ts"
    },
    "./tools": {
      "import": "./src/tools/index.ts",
      "types": "./build/tools/index.d.ts"
    },
    "./travel": {
      "import": "./src/travel/index.ts",
      "types": "./build/travel/index.d.ts"
    },
    "./types": {
      "import": "./src/types.ts",
      "types": "./build/types.d.ts"
    },
    "./vector": {
      "import": "./src/vector.ts",
      "types": "./build/vector.d.ts"
    },
    "./chat": {
      "import": "./src/chat/index.ts",
      "types": "./build/chat/index.d.ts"
    },
    "./user": {
      "import": "./src/users/index.ts",
      "types": "./build/users/index.d.ts"
    },
    "./package.json": "./package.json"
  },
  "types": "./build/index.d.ts",
  "scripts": {
    "build": "tsc -b",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate-test": "DATABASE_URL=postgres://postgres:postgres@localhost:4433/hominem-test drizzle-kit migrate",
    "db:update": "drizzle-kit generate && drizzle-kit migrate",
    "dev": "tsc -d -w",
    "lint": "biome lint ./src --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "format": "biome format ./src --fix"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@hominem/utils": "workspace:*",
    "ai": "^4.3.16",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "bullmq": "5.66.4",
    "dotenv": "17.2.3",
    "drizzle-zod": "^0.7.0",
    "effect": "^3.16.10",
    "googleapis": "^144.0.0",
    "google-auth-library": "9.15.1",
    "langchain": "^0.3.15",
    "mammoth": "^1.9.1",
    "openai": "4.83.0",
    "pdf2json": "^3.1.6",
    "postgres": "^3.4.5",
    "resend": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "4.2.1",
    "sharp": "^0.32.4"
  },
  "devDependencies": {
    "@hominem/tsconfigs": "workspace:*",
    "@supabase/supabase-js": "2.88.0",
    "@types/uuid": "^10.0.0",
    "drizzle-orm": "0.45.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
