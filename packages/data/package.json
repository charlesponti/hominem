{
	"name": "@hominem/data",
	"version": "0.0.0",
	"description": "Database schemas and utilities",
	"type": "module",
	"sideEffects": false,
	"exports": {
		"./db": "./src/db/index.ts",
		"./emails": "./src/resend.ts",
		"./events": "./src/events/index.ts",
		"./files": "./src/files.ts",
		"./finance": "./src/finance/index.ts",
		"./fixtures": "./src/fixtures.ts",
		"./google-calendar": "./src/google.ts",
		"./health": "./src/health/index.ts",
		"./jobs": "./src/jobs/index.ts",
		"./lists": "./src/lists/index.ts",
		"./logger": "./src/logger.ts",
		"./places": "./src/places.ts",
		"./notes": "./src/notes/index.ts",
		"./schema": "./src/db/schema/index.ts",
		"./services": "./src/services/index.ts",
		"./queues": "./src/queues.ts",
		"./redis": "./src/redis.ts",
		"./tools": "./src/tools/index.ts",
		"./travel": "./src/travel/index.ts",
		"./types": "./src/types.ts",
		"./vector": "./src/vector.ts",
		"./chat": "./src/chat/index.ts",
		"./user": "./src/users/index.ts",
		"./package.json": "./package.json"
	},
	"scripts": {
		"typecheck": "tsc --noEmit",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate-test": "DATABASE_URL=\"postgres://postgres:postgres@localhost:4433/hominem-test?options=-c%20client_min_messages%3Dwarning\" drizzle-kit migrate",
		"db:update": "drizzle-kit generate && drizzle-kit migrate",
		"dev": "tsc -d -w",
		"format": "bunx oxfmt ./src",
		"lint": "bunx oxlint ./src",
		"test": "vitest run --passWithNoTests",
		"test:watch": "vitest"
	},
	"dependencies": {
		"@ai-sdk/google": "^1.2.12",
		"@hominem/utils": "workspace:*",
		"ai": "^4.3.16",
		"better-auth": "^1.4.12",
		"bullmq": "5.66.4",
		"csv-parse": "^5.6.0",
		"csv-parser": "^3.2.0",
		"dotenv": "17.2.3",
		"drizzle-orm": "0.45.1",
		"drizzle-zod": "^0.7.0",
		"effect": "^3.16.10",
		"googleapis": "^170.0.0",
		"ioredis": "^5.9.1",
		"langchain": "^0.3.15",
		"mammoth": "^1.9.1",
		"openai": "4.83.0",
		"pdf2json": "^3.1.6",
		"postgres": "^3.4.5",
		"resend": "^4.0.1",
		"sharp": "^0.32.4",
		"uuid": "^11.1.0",
		"zod": "4.2.1"
	},
	"devDependencies": {
		"@supabase/supabase-js": "2.88.0",
		"@types/uuid": "^10.0.0",
		"typescript": "5.9.3",
		"vitest": "4.0.16"
	}
}
