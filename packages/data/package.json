{
  "name": "@hominem/data",
  "version": "0.0.0",
  "type": "module",
  "sideEffects": false,
  "description": "Database schemas and utilities",
  "exports": {
    ".": "./src/index.ts",
    "./db": "./src/db/index.ts",
    "./emails": "./src/resend.ts",
    "./files": "./src/files.ts",
    "./finance": "./src/finance/index.ts",
    "./fixtures": "./src/fixtures.ts",
    "./lists": "./src/lists/index.ts",
    "./logger": "./src/logger.ts",
    "./places": "./src/places.ts",
    "./schema": "./src/db/schema/index.ts",
    "./services": "./src/services/index.ts",
    "./tools": "./src/tools/index.ts",
    "./types": "./src/types.ts",
    "./vector": "./src/vector.ts",
    "./chat": "./src/chat/index.ts",
    "./user": "./src/services/user-auth.service.ts"
  },
  "scripts": {
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate-test": "DATABASE_URL=postgres://postgres:postgres@localhost:4433/hominem-test drizzle-kit migrate",
    "db:update": "drizzle-kit generate && drizzle-kit migrate",
    "lint": "biome lint ./src --fix",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest",
    "format": "biome format ./src --fix"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@hominem/utils": "workspace:*",
    "ai": "^4.3.16",
    "csv-parse": "^5.6.0",
    "csv-parser": "^3.2.0",
    "dotenv": "17.2.3",
    "drizzle-zod": "^0.7.0",
    "effect": "^3.16.10",
    "googleapis": "^144.0.0",
    "google-auth-library": "9.15.1",
    "langchain": "^0.3.15",
    "mammoth": "^1.9.1",
    "openai": "4.83.0",
    "pdf2json": "^3.1.6",
    "pino": "^9.7.0",
    "postgres": "^3.4.5",
    "resend": "^4.0.1",
    "uuid": "^11.1.0",
    "zod": "4.2.1"
  },
  "devDependencies": {
    "@hominem/tsconfigs": "workspace:*",
    "@supabase/supabase-js": "2.88.0",
    "@types/uuid": "^10.0.0",
    "drizzle-orm": "0.45.1",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  }
}
