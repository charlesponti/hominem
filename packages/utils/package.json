{
  "name": "@hominem/utils",
  "version": "0.0.0",
  "description": "Random utilities",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./supabase": "./src/supabase/index.ts",
    "./consts": "./src/consts.ts",
    "./dates": "./src/dates.ts",
    "./images": "./src/images.ts",
    "./google": "./src/google.ts",
    "./http": "./src/http.ts",
    "./location": "./src/location/index.ts",
    "./logger": "./src/logger.ts",
    "./markdown": "./src/markdown/index.ts",
    "./schemas": "./src/schemas/index.ts",
    "./scraping": "./src/scraping/index.ts",
    "./time": "./src/time.ts",
    "./types/change-management": "./src/types/change-management.ts"
  },
  "scripts": {
    "build": "tsc -d",
    "clean": "rm -rf build",
    "typecheck": "tsc --noEmit",
    "format": "bunx oxfmt ./src",
    "lint": "bunx oxlint ./src",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.2.12",
    "@ai-sdk/openai": "^1.3.22",
    "@supabase/storage-js": "^2.7.3",
    "@supabase/supabase-js": "2.88.0",
    "ai": "^4.3.16",
    "axios": "1.12.0",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "pino": "^9.7.0",
    "playwright": "^1.51.0",
    "zod": "4.3.6"
  },
  "devDependencies": {
    "jsdom": "^26.0.0",
    "typescript": "5.9.3",
    "vfile": "^6.0.3",
    "vitest": "4.0.16"
  }
}
