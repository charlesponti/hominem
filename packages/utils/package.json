      "types": "./build/schemas/index.d.ts",
      "development": "./src/schemas/index.ts",
      "default": "./build/schemas/index.js"
    },
    "./scraping": {
      "types": "./build/scraping/index.d.ts",
      "development": "./src/scraping/index.ts",
      "default": "./build/scraping/index.js"
