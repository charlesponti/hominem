{
  "name": "@hominem/utils",
  "version": "0.0.0",
  "description": "Random utilities",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "development": "./src/index.ts",
      "default": "./build/index.js"
    },
    "./supabase": {
      "types": "./build/supabase/index.d.ts",
      "development": "./src/supabase/index.ts",
      "default": "./build/supabase/index.js"
    },
