      "types": "./build/types/change-management.d.ts",
      "development": "./src/types/change-management.ts",
      "default": "./build/types/change-management.js"
    }
  },
  "scripts": {
    "build": "tsc -d",
    "typecheck": "tsc --noEmit",
    "format": "bunx oxfmt ./src",
    "lint": "bunx oxlint ./src",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@ai-sdk/google": "^1.1.10",
    "@ai-sdk/openai": "^1.1.9",
    "@langchain/core": "^1.1.16",
    "@langchain/textsplitters": "^1.0.1",
    "@supabase/storage-js": "^2.7.3",
    "@supabase/supabase-js": "2.88.0",
    "ai": "^4.3.16",
    "axios": "1.12.0",
    "cheerio": "^1.0.0",
    "compromise": "^14.14.4",
    "csvtojson": "^2.0.10",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "dotenv": "17.2.3",
