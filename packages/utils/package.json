      "default": "./build/images.js"
    },
    "./google": {
      "types": "./build/google.d.ts",
      "development": "./src/google.ts",
      "default": "./build/google.js"
    },
    "./http": {
      "types": "./build/http.d.ts",
      "development": "./src/http.ts",
      "default": "./build/http.js"
    },
    "./location": {
      "types": "./build/location/index.d.ts",
      "development": "./src/location/index.ts",
      "default": "./build/location/index.js"
    },
    "./logger": {
      "types": "./build/logger.d.ts",
      "development": "./src/logger.ts",
      "default": "./build/logger.js"
    },
    "./markdown": {
      "types": "./build/markdown/index.d.ts",
      "development": "./src/markdown/index.ts",
      "default": "./build/markdown/index.js"
