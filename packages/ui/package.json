{
  "name": "@hominem/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "development": "./src/index.ts",
      "default": "./build/index.js"
    },
    "./button": {
      "types": "./build/components/ui/button.d.ts",
      "default": "./build/components/ui/button.js"
    },
    "./input": {
      "types": "./build/components/ui/input.d.ts",
      "default": "./build/components/ui/input.js"
    },
    "./label": {
      "types": "./build/components/ui/label.d.ts",
      "default": "./build/components/ui/label.js"
    },
    "./textarea": {
      "types": "./build/components/ui/textarea.d.ts",
      "default": "./build/components/ui/textarea.js"
    },
    "./dialog": {
      "types": "./build/components/ui/dialog.d.ts",
      "default": "./build/components/ui/dialog.js"
    },
    "./dropdown": {
      "types": "./build/components/ui/dropdown-menu.d.ts",
      "default": "./build/components/ui/dropdown-menu.js"
    },
    "./card": {
      "types": "./build/components/ui/card.d.ts",
      "default": "./build/components/ui/card.js"
    },
    "./loading": {
      "types": "./build/components/ui/loading.d.ts",
      "default": "./build/components/ui/loading.js"
    },
    "./list": {
      "types": "./build/components/list.d.ts",
      "default": "./build/components/list.js"
    },
    "./filters": {
      "types": "./build/components/filters/index.d.ts",
      "default": "./build/components/filters/index.js"
    },
    "./hooks": {
      "types": "./build/hooks/index.d.ts",
      "default": "./build/hooks/index.js"
    },
    "./components/ui/*": {
      "types": "./build/components/ui/*.d.ts",
      "default": "./build/components/ui/*.js"
    },
    "./components/*": {
      "types": "./build/components/*.d.ts",
      "default": "./build/components/*.js"
    },
    "./hooks/*": {
      "types": "./build/hooks/*.d.ts",
      "default": "./build/hooks/*.js"
    },
    "./lib/*": {
      "types": "./build/lib/*.d.ts",
      "default": "./build/lib/*.js"
    },
    "./styles/*": "./src/styles/*"
  },
  "scripts": {
    "build": "tsc -b",
    "format": "bunx oxfmt ./src",
    "lint": "bunx oxlint ./src",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --passWithNoTests",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@hominem/auth": "workspace:*",
    "@hominem/utils": "workspace:*",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "2.88.0",
    "@tanstack/react-query": "5.90.16",
    "axios": "1.12.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "lucide-react": "0.562.0",
    "react": "^19.2.3",
    "react-day-picker": "8.10.1",
    "react-hook-form": "^7.70.0",
    "react-router": "7.12.0",
    "tailwind-merge": "3.2.0",
    "tailwindcss": "^4.1.18",
    "vaul": "^1.1.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@vitejs/plugin-react": "5.1.2",
    "react": "^19.2.3",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vite-tsconfig-paths": "6.0.3",
    "vitest": "4.0.16"
  },
  "peerDependencies": {
    "react": "^19.2.3"
  }
}
