{
  "name": "@hominem/db",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./schema": "./src/schema/index.ts",
    "./schema/notes": "./src/schema/notes.schema.ts",
    "./schema/content": "./src/schema/content.schema.ts",
    "./schema/finance": "./src/schema/finance.schema.ts",
